{
  "name": "dashboard",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --fix --max-warnings=0",
    "lint:check": "eslint . --max-warnings=0",
    "preview": "vite preview",
    "test": "vitest run",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prettier": "prettier --check .",
    "prettier:write": "prettier --write .",
    "knip": "knip",
    "knip:production": "knip --production",
    "types": "tsc --noEmit",
    "check": "npm run test:run && npm run build && npm run lint && npm run knip && npm run types && npm run prettier:write",
    "upgrade": "npm update && npm audit fix",
    "audit:fix": "npm audit fix",
    "audit:force": "npm audit fix --force",
    "healthcheck": "node scripts/healthcheck.cjs",
    "password": "node scripts/generate-password.js"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.12",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.7",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "bcryptjs": "^3.0.2",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.7.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "recharts": "^3.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@lhci/cli": "^0.15.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.7",
    "@types/luxon": "^3.6.2",
    "@types/node": "^24.0.13",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.3.0",
    "jsdom": "^24.0.0",
    "knip": "^5.0.0",
    "msw": "^2.12.4",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.36.0",
    "vite": "^7.0.4",
    "vitest": "^3.2.4",
    "wait-on": "^8.0.3"
  }
